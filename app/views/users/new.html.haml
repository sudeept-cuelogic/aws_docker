%h2
  New User
= link_to 'Users', users_path
= form_for @user do |f|
  = f.radio_button :service, 'search', checked: true
  = f.label :elastic_search
  = f.radio_button :service, 'cache'
  = f.label :elastic_cache
  %br
  = f.label :first_name
  = f.text_field :first_name
  %br
  = f.label :last_name
  = f.text_field :last_name
  %br
  = f.label :email
  = f.text_field :email
  %br
  = f.label :birth_date
  = f.text_field :dob
  = f.submit
